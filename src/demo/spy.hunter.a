         sty   $0D93
         sty   $E95
         sty   $EB4
         sty   $61C7
         lda   #$EA
         sta   $433
         sta   $434
         sta   $2C5E
         sta   $2C5F
         lda   #$2C
         sta   $7AB
         lda   #3
         sta   $7AA
         sta   $7CF
         lda   #$1A
         sta   $E78
         lda   #$12
         sta   $E80
         lda   #$60
         sta   $E92
         lda   #$4C
         sta   $E93
         sta   $EB2
         sta   $61C5

         +DISABLE_ACCEL

         jmp   $0451

title    !byte x_e-x_b
x_b      !text "SPY.HUNTER"
x_e

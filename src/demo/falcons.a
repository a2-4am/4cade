!cpu 6502
!to "build/DEMO/FALCONS#060200",plain
*=$200

         !source "src/constants.a"   ; no code in these
         !source "src/macros.a"

         +LOAD_XSINGLE title

         lda   #$60
         sta   $85F
         jsr   $800       ; decompress

         lda   #$2C
         sta   $602A      ; skip input selection

         ldx   #3-1
-        lda   patch,x
         sta   $9FF7,x
         sta   $9FDC,x    ; set exits
         dex
         bpl   -

         jmp   $6000

patch    !byte $4C,$00,$01

title    +PSTRING "FALCONS"

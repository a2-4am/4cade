!cpu 6502
!to "build/PRELAUNCH.INDEXED/COLORIX",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4013
         jsr   $4000      ; decompress
         ldx   #0
         stx   $2539
         inx
         stx   $253A      ; allow quit
         lda   #$96
         sta   $74        ; Basic hook
         +DISABLE_ACCEL

         jmp   $800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

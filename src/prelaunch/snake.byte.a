!cpu 6502
!to "build/PRELAUNCH.INDEXED/SNAKE.BYTE",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #$60
         sta   $0858
         jsr   $0800      ; decompress
         +LC_REBOOT
         lda   #0
         sta   $7a11
         lda   #1
         sta   $7a19
         jmp   $7680

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/OUTPOST",plain
*=$106

    !source "src/macros.a"

         +HIDE_ARTWORK
         ;+ENABLE_ACCEL   ; can't
         lda   #$60
         sta   $3EF2
         jsr   $3EC8      ; decompress/title/set reset vector

         +RESET_VECTOR $100
         lda   #$60
         sta   $211
         jsr   $200
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$B5
         sta   $8086      ; patch - don't decrease lives
+
         jmp   $8000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

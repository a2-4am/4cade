!cpu 6502
!to "build/PRELAUNCH/MOON.PATROL",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $49d5
         jsr   $800
         lda   #$60
         sta   $2fe
         jsr   $2ef
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $2d07
         sta   $1c17
+
         ldy   #6
-        lda   $100-1,y   ; game uses $100 as variable
         sta   $1B0-1,y   ; move hook to $1B0
         dey
         bne   -

         lda   #$B0
         sta   $3F2
         lda   #$01
         sta   $3F3
         lda   #$A4
         sta   $3F4       ; reset vector patch

         +DISABLE_ACCEL
         jmp   $919

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

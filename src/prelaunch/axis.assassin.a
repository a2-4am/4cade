!cpu 6502
!to "build/PRELAUNCH/AXIS.ASSASSIN",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3f06
         jsr   $3eb1      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6d31
+
         +DISABLE_ACCEL
         jmp   $5c00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/AXIS.ASSASSIN",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #$60
         sta   $3f06
         jsr   $3eb1      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6d31
+
         lda   #$4C
         sta   $5B2E
         lda   #0
         sta   $5B2F
         lda   #1
         sta   $5B30      ; reset vector patch

         jmp   $5c00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

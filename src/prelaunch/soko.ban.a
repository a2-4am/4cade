!cpu 6502
!to "build/PRELAUNCH.INDEXED/SO",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $A17
         jsr   $800       ; load "DOS"
         lda   #$60
         sta   $EC38
         jsr   $EC00      ; set up vectors
         inc   $3F4       ; reboot
         jsr   DisableAccelerator
         jsr   HideLaunchArtworkLC2
         jmp   $F6C5

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

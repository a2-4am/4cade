!cpu 6502
!to "build/PRELAUNCH.INDEXED/HIGH.RISE",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $0836
         jsr   $0800      ; decompress

         lda   #$2C
         sta   $44AA
         sta   $44AD
         sta   $44B0      ; reset vector fix
         
         +DISABLE_ACCEL

         jmp   $449A

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

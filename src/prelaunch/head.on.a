!cpu 6502
!to "build/PRELAUNCH/HEAD.ON",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #$2C
         sta   $7000
         lda   #$88
         sta   $7001
         lda   #$C0
         sta   $7002
         lda   #$4C
         sta   $7003
         lda   #<RELBASE  ; low byte
         sta   $7004
         lda   #>RELBASE  ; high byte
         sta   $7005      ; re-establish LC hook

         lda   #0
         sta   $3F2
         lda   #$70
         sta   $3F3
         lda   #$D5
         sta   $3F4       ; reset vector patch

         jmp   $2000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

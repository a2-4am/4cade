!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPACE.WARRIOR",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4031
         jsr   $4000      ; decompress
         sta   $2000
         +DISABLE_ACCEL
         jmp   $4034

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/ORILEYS.MINE",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $81D
         jsr   $800       ; decompress

         lda   #1
         sta   $2006
         sta   $201B      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $8FD

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

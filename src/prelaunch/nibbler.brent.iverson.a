!cpu 6502
!to "build/PRELAUNCH.INDEXED/NB",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $874
         jsr   $800       ; decompress

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $673A      ; patch - don't decrease lives
+
         lda   #$A0
         sta   $400       ; hide activity monitor once loaded
         +DISABLE_ACCEL_LC
         jmp   $877

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

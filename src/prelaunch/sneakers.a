!cpu 6502
!to "build/PRELAUNCH.INDEXED/SNEAKERS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL_AND_HIDE_ARTWORK
         lda   #$60
         sta   $84c7
         jsr   $3ffa      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6ed8
         sta   $7f1c
+
         lda   #1
         sta   $9E2F      ; reset vector patch

         +DISABLE_ACCEL_LC
         jmp   $9e00

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

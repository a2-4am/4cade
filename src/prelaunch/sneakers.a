!cpu 6502
!to "build/PRELAUNCH/SNEAKERS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $84c8
         jsr   $3ffa      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6ed8
         sta   $7f1c
+
         lda   #1
         sta   $9E2F      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $9e00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/PITFALL.II",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3D34
         jsr   $0800      ; decompress
         +RESET_VECTOR reset
         +DISABLE_ACCEL
         jmp   $6000

reset
         +READ_RAM2_NO_WRITE
         jmp   ($FFFC)

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

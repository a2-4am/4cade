!cpu 6502
!to "build/PRELAUNCH.INDEXED/CLAM.BAKE",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $1825
         jsr   $1800      ; decompress

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $7C6B      ; patch - don't decrease lives
         sta   $CDC       ; patch - don't increase lives
+
         +DISABLE_ACCEL_LC
         jmp   $7800

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

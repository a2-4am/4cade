!cpu 6502
!to "build/PRELAUNCH/THE.DAM.BUSTERS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9B3
         jsr   $800       ; decompress

         jsr   DisableAccelerator
         jsr   $BE44
         +LC_REBOOT
         jmp   $9B6

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/FIREBUG",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $2076
         jsr   $2000      ; decompress

         lda   #0
         sta   $83E
         lda   #1
         sta   $849       ; reset vector fix

         +DISABLE_ACCEL
         jmp   $810

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
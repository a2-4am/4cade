!cpu 6502
!to "build/PRELAUNCH.INDEXED/STAR.MAZE",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $21FD
         jsr   $2000      ; decompress
         sta   $7A07
         sty   $7A08      ; reset vector

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $87D0
+
         +DISABLE_ACCEL
         jmp   $79D3

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

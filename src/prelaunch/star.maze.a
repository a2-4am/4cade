!cpu 6502
!to "build/PRELAUNCH.INDEXED/STAR.MAZE",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$E2
         sta   $5F07
         sta   $5F0D

         lda   #$60
         sta   $21FD
         jsr   $2000      ; decompress
         sta   $7A07
         sty   $7A08      ; reset vector

         lda   #$D3
         sta   $7E52
         dec   $7E53
         ldx   #(patch_e-patch_b)-1
-        lda   patch_b, x
         sta   $7ED9, x
         dex
         bpl   -

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $87D0
+
         jsr   $7F8B      ; swap screen holes
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jsr   $7F8B      ; swap screen holes
         jmp   $79D3

patch_b  +READ_ROM_NO_WRITE
         lda   #$2C
         sta   $7ED6
         rts
patch_e

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

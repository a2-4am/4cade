!cpu 6502
!to "build/PRELAUNCH/PITSTOP.II",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #$60
         sta   $4055
         jsr   $4000      ; decompress
         +LC_REBOOT
         lda   #$2c
         sta   $b12
         sta   $b17       ; don't let game change reset vector
         jmp   $a00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

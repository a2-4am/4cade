!cpu 6502
!to "build/PRELAUNCH.INDEXED/TORAX",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2086
         jsr   $2000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6183      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         ldy   #$20
         jmp   $2088

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/CANYON.CLIMBER",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8022
         jsr   $67de      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $35c2
+
         +DISABLE_ACCEL
         jmp   $3000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

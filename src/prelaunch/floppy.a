!cpu 6502
!to "build/PRELAUNCH.INDEXED/FLOPPY",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $1AED
         jsr   $18B0      ; decompress
         lda   #1
         sta   $42AF

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $4400      ; patch - don't increase lives
         sta   $46E0      ; patch - don't decrease lives
         lda   #0
         sta   $4255      ; patch - don't decrease time
+
         +DISABLE_ACCEL_LC

         jmp   $4000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/DD.RETURNS",plain
*=$106

         !source "src/macros.a"

         lda   #$60
         sta   $336
         jsr   $300
         +ENABLE_ACCEL

         lda   #$60
         sta   $2035
         sta   $20A7
         ldy   #(x_e - x_b) - 1
-        lda   x_b, y
         sta   $200, y
         dey
         bpl   -
         jmp   $200

x_b !pseudopc $200 {
         jsr   $2000      ; ProDOS init
         tay
         sta   $c009
         jsr   $2036      ; decompress
         sta   $c008
         +RESET_VECTOR $873
         +DISABLE_ACCEL
         sta   $c009
         jmp   $800
}
x_e

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/ELECTROARENA",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $7252
         jsr   $3F78      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         dec   $6190      ; don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

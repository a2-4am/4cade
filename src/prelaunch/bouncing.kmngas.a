!cpu 6502
!to "build/PRELAUNCH.INDEXED/BK",plain
*=$106

         !source "src/prelaunch/common.a"

         +READ_RAM2_WRITE_RAM2
         jsr   EnableAccelerator
         +LOAD_FILE_AT kamungas_mb, $200
         +READ_ROM_NO_WRITE
         jmp   $200

kamungas_mb
         !byte 21
         !text "PRELAUNCH/KAMUNGAS.MB"

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

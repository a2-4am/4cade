!cpu 6502
!to "build/PRELAUNCH/MARIO.BROS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8214
         jsr   $4900      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a9
         sta   $4299
+
         +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/MARIO.BROS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8240
         jsr   $4900      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a9
         sta   $4299
+
         lda   #1
         sta   $402F      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

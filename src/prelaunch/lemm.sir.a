!cpu 6502
!to "build/PRELAUNCH.INDEXED/LEMM.SIR",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $41B0
         jsr   $4000      ; decompress
         inc   $C9        ; RNG requires non-zero
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $BA5E      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $8C8

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

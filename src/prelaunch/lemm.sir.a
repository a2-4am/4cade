!cpu 6502
!to "build/PRELAUNCH.INDEXED/LEMM.SIR",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $41B0
         jsr   $4000      ; decompress
         inc   $C9        ; RNG requires non-zero
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $BA5E      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $8C8

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

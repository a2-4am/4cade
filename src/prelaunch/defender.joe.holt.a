!cpu 6502
!to "build/PRELAUNCH/DJ",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $202F
         jsr   $2000      ; decompress
         lda   #$88
         sta   $E001
         jsr   DisableAccelerator
         jmp   $200

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

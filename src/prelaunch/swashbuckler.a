!cpu 6502
!to "build/PRELAUNCH.INDEXED/SWASHBUCKLER",plain
*=$106

    !source "src/prelaunch/common.a"

         ;+ENABLE_ACCEL   ; can't
         lda   #$60
         sta   $332C      ; stop1
         jsr   $330F      ; decompress

         lda   #$60
         sta   $BC81      ; stop2
         jsr   $BC00      ; title

         +NEW_RESET_VECTOR $BFF0

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $115A      ; patch - don't increase lives
         sta   $136B      ; patch - don't decrease lives
+
         jmp   $1B34

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

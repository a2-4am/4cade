!cpu 6502
!to "build/PRELAUNCH/SPACE.RAIDERS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8F09
         jsr   $8EF1      ; decompress

         lda   #1
         sta   $401A      ; reset vector patch
         +DISABLE_ACCEL

         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

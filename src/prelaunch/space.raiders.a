!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPACE.RAIDERS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8F09
         jsr   $8EF1      ; decompress

         lda   #1
         sta   $401A      ; reset vector patch
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $4000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

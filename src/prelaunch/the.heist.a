!cpu 6502
!to "build/PRELAUNCH.INDEXED/THE.HEIST",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #<callback
         sta   $407F
         lda   #>callback
         sta   $4080
         jmp   $4000      ; decompress

callback
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $A05

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

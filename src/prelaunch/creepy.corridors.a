!cpu 6502
!to "build/PRELAUNCH.INDEXED/CREEPY.CRRIDORS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1D21
         jsr   $1D00      ; decompress title
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $AC6        ; patch - don't decrease lives
         sta   $B3E        ; patch - don't increase lives
+
         +DISABLE_ACCEL_LC
         jmp   $800

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

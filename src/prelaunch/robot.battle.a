!cpu 6502
!to "build/PRELAUNCH.INDEXED/ROBOT.BATTLE",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $82F
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$bd
         sta   $7930      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

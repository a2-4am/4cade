!cpu 6502
!to "build/PRELAUNCH.INDEXED/RETRO.FEVER",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $813
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $4817      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC

         jmp   $4000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

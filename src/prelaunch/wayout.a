!cpu 6502
!to "build/PRELAUNCH.INDEXED/WAYOUT",plain
*=$106

    !source "src/macros.a"

         +USES_TEXT_PAGE_2
         +ENABLE_ACCEL
         lda   #$60
         sta   $206A
         jsr   $2000      ; decompress
         jsr   HideLaunchArtwork
         lda   #$60
         sta   $97DE
         jsr   $979B      ; decompress more
         lda   #$60
         sta   $8D75
         jsr   $8D53      ; decompress even more
         lda   #$20
         sta   $8D75
         +RESET_VECTOR $100
         +DISABLE_ACCEL
         jmp   $8D75

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

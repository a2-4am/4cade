!cpu 6502
!to "build/PRELAUNCH.INDEXED/FALCONS",plain
*=$106

    !source "src/macros.a"

         +HIDE_ARTWORK
         lda   #$60
         sta   $85f
         jsr   $800       ; decompress
         ldx   #$CA
         stx   $944F
         inx
         stx   $9457
         inx
         stx   $9453
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         dec   $9d19      ; patch - don't decrease lives
         dec   $9d06      ; patch - don't decrease lives
+
         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

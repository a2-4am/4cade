!cpu 6502
!to "build/PRELAUNCH.INDEXED/TIME.TUNNELS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2084
         jsr   $2000       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $64B2       ; patch - don't increase lives
         sta   $64C7       ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $828

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/TIME.TUNNELS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2084
         jsr   $2000       ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $64B2       ; patch - don't increase lives
         sta   $64C7       ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $828

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

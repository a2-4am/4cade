!cpu 6502
!to "build/PRELAUNCH/VORTEX",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #<hook
         sta   $407E+1
         lda   #>hook
         sta   $407E+2
         jmp   $4000      ; decompress2

hook     +NEW_RESET_VECTOR $BFF0
         +DISABLE_ACCEL
         jmp   $D566

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
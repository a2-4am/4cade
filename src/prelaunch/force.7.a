!cpu 6502
!to "build/PRELAUNCH.INDEXED/FORCE.7",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL_AND_HIDE_ARTWORK
         inc   $3F4        ; 128k - must reboot
         lda   #<callback
         sta   $556B
         lda   #>callback
         sta   $556C
         lda   #$4C
         sta   $5581
         jsr   $2A55       ; show title screen
         lda   #<reset
         sta   $121A
         lda   #$A6
         sta   $1190
         lda   #$FA
         sta   $1191
         jmp   $1000

reset
         +READ_ROM_NO_WRITE
         jmp   $FAA6

callback
         jsr   DisableAccelerator
         jsr   $800
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $E263
         sta   $E266
         sta   $E269
         sta   $E26C
         sta   $E95B
+        rts

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

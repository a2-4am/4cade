!cpu 6502
!to "build/PRELAUNCH/CONAN",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $81e
         jsr   $800
         lda   #$60
         sta   $bd40
         jsr   $bc94
         +DISABLE_ACCEL
         jsr   $5ff8     ; run intro

         +ENABLE_ACCEL

         ; re-patch reset vector (doesn't work during copyright sequence)
         +RESET_VECTOR $100

         lda   #$6c
         sta   $bd40
         lda   #$60
         sta   $6ba2
         jsr   $3ffc
         jsr   $be00
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $19f5
         sta   $1a00
         sta   $1b4e
         ;;sta $a973
+
         +DISABLE_ACCEL
         jmp   $6ba5

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

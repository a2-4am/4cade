!cpu 6502
!to "build/PRELAUNCH.INDEXED/PICNIC.PARANOIA",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #<callback
         sta   $450d
         lda   #>callback
         sta   $450a
         jmp   $4500

callback
         nop
         lda   #0
         sta   $45e5
         lda   #1
         sta   $45ea
         eor   #$A5
         sta   $45ef

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         dec   $79B7      ; patch - don't decrease time
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $78fb

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

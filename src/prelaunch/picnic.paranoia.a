!cpu 6502
!to "build/PRELAUNCH/PICNIC.PARANOIA",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #<callback
         sta   $450d
         lda   #>callback
         sta   $450a
         jmp   $4500

callback
         nop
         +DISABLE_ACCEL
         lda   #0
         sta   $45e5
         lda   #1
         sta   $45ea
         eor   #$A5
         sta   $45ef
         jmp   $78fb

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

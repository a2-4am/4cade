!cpu 6502
!to "build/PRELAUNCH.INDEXED/GALAXY.GATES",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $6013
         jsr   $6000      ; decompress

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $20F3      ; patch - don't decrease lives
         sta   $20BB      ; patch - don't increase lives
+        +DISABLE_ACCEL_LC
         jmp   $3A00

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

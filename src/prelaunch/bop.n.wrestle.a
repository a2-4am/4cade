!cpu 6502
!to "build/PRELAUNCH/BOP.N.WRESTLE",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9F4
         jsr   $800       ; load "DOS"

         +READ_RAM2_WRITE_RAM2
         jsr   DisableAccelerator
         +LC_REBOOT
         +READ_ROM_NO_WRITE
         jmp   $B700

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

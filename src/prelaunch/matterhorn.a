!cpu 6502
!to "build/PRELAUNCH.INDEXED/MS",plain
*=$106

    !source "src/macros.a"

           +HIDE_ARTWORK
           +GET_MACHINE_STATUS
           and   #CHEATS_ENABLED
           beq   +
           lda   #$2C
           sta   $5255      ; do not decrement lives
+
           jmp   $2000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

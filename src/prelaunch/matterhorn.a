!cpu 6502
!to "build/PRELAUNCH.INDEXED/MS",plain
*=$106

    !source "src/macros.a"

           +READ_RAM2_NO_WRITE
           jsr   HideLaunchArtworkLC2
           lda   MachineStatus
           +READ_ROM_NO_WRITE
           and   #CHEATS_ENABLED
           beq   +
           lda   #$2C
           sta   $5255      ; do not decrement lives
+
           jmp   $2000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

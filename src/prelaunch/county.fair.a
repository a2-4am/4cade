!cpu 6502
!to "build/PRELAUNCH/COUNTY.FAIR",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $65AA
         jsr   $651F      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $1060      ; patch - don't decrease ammunition
+
         +DISABLE_ACCEL
         jmp   $800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

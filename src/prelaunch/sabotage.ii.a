!cpu 6502
!to "build/PRELAUNCH.INDEXED/SABOTAGE.II",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $942F
         jsr   $7300      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $1529       ; patch - don't decrease lives
         sta   $156D       ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $1300

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

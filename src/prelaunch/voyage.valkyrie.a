!cpu 6502
!to "build/PRELAUNCH.INDEXED/VV",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         +NEW_RESET_VECTOR $180

         lda   #<callback
         sta   $A52
         lda   #>callback
         sta   $A53
         jmp   $800       ; load "DOS"

callback
         ldx   #$80
         stx   $9E52
         ldx   #$01
         stx   $9E53

         +DISABLE_ACCEL
         jmp   $9D84

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

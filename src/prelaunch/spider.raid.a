!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPIDER.RAID",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $3F0E
         jsr   $3EB9      ; decompress

         lda   #1
         sta   $40D3      ; reset vector

         lda   #$60
         sta   $40F1
         jsr   $4000

         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         dec   $976A      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $4246

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

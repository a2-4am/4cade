!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPIDER.RAID",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3F0E
         jsr   $3EB9      ; decompress
         lda   #$60
         sta   $40F1
         jsr   $4000
         +RESET_VECTOR $100

         +DISABLE_ACCEL
         jmp   $4246

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/MINOTAUR",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $21BB
         jsr   $2000      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$37
         sta   $180       ; patch - enable cheat mode
+
         +DISABLE_ACCEL
         jmp   $FF8

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

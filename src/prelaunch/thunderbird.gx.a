!cpu 6502
!to "build/PRELAUNCH.INDEXED/THUNDERBIRD.GX",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $123A
         jsr   $1200       ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $4F4        ; patch - don't decrease lives
         sta   $B2A        ; patch - don't increase lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $E12

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/DINO.SMURF",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2183
         jsr   $2100      ; decompress

         lda   ROM_MACHINEID
         cmp   #$06
         beq   ++
         ldy   #$2C
-        lda   $AE4A,Y
         cmp   #$E1
         bcc   +
         and   #$DF
         sta   $AE4A,Y
+        dey
         bne   -

++       +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $1895      ; patch - don't decrease lives

+        +DISABLE_ACCEL_LC
         jmp   $A892

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

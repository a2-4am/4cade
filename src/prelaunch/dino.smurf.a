!cpu 6502
!to "build/PRELAUNCH/DINO.SMURF",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2183
         jsr   $2100      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $1895      ; patch - don't decrease lives

+        lda   ROM_MACHINEID
         cmp   #$06
         beq   ++
         ldy   #$2C
-        lda   $AE4A,Y
         cmp   #$E1
         bcc   +
         and   #$DF
         sta   $AE4A,Y
+        dey
         bne   -

++       +DISABLE_ACCEL
         jmp   $A892

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

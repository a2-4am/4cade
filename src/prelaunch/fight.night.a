!cpu 6502
!to "build/PRELAUNCH/FIGHT.NIGHT",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $A21
         jsr   $800       ; load "DOS"

         +READ_RAM1_WRITE_RAM1
         +LC_REBOOT
         +DISABLE_ACCEL
         jmp   $B700

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

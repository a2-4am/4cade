!cpu 6502
!to "build/PRELAUNCH.INDEXED/DEATH.SWORD",plain
*=$106

    !source "src/macros.a"

           +ENABLE_ACCEL
           +NEW_RESET_VECTOR_64K $300
           lda   #$8b
           sta   $bf0e      ; disable ROM mapping
           lda   #$60
           sta   $921
           jsr   $800       ; decompress
           lda   $c083
           jsr   DisableAccelerator
           lda   $c08b
           jmp   $6000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

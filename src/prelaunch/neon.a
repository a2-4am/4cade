!cpu 6502
!to "build/PRELAUNCH/NEON",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1D7B
         jsr   $1D00      ; decompress

         lda   #0
         sta   $149B
         lda   #1
         sta   $14A0      ; reset vector fix

         +DISABLE_ACCEL
         jmp   $149A

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
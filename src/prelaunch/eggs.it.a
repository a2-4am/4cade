!cpu 6502
!to "build/PRELAUNCH.INDEXED/EI",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3E98
         sta   $3EF2
         jsr   HideLaunchArtwork
         jsr   $3DE9      ; decompress & show title
         +DISABLE_ACCEL
         jsr   $4C00
         +ENABLE_ACCEL
         ldy   #0
         jsr   $B839
         +DISABLE_ACCEL
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
                          ; lives in $7801
         lda   #$AD
         sta   $962A      ; patch - don't decrease lives
+
         jmp   ($0020)

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

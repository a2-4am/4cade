!cpu 6502
!to "build/PRELAUNCH.INDEXED/ZENITH",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL_AND_HIDE_ARTWORK
         lda   #$60
         sta   $403E
         jsr   $4000      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$60
         sta   $A235      ; patch - don't decrease fuel
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE

         jmp   ($20)

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/ZENITH",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL_AND_HIDE_ARTWORK
         lda   #$60
         sta   $403E
         jsr   $4000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$60
         sta   $A235      ; patch - don't decrease fuel
+
         +DISABLE_ACCEL_LC

         jmp   ($20)

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

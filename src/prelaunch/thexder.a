!cpu 6502
!to "build/PRELAUNCH/THEXDER",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $13A8
         jsr   $1200

         +RESET_VECTOR reset
         lda   #<reset
         sta   $47FC
         lda   #>reset
         sta   $47FD      ; moved to LC $FFFC/D

         +DISABLE_ACCEL
         jmp   $1800

reset
         +READ_ROM_NO_WRITE
         jmp   $FAA6

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

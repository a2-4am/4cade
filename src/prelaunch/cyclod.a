!cpu 6502
!to "build/PRELAUNCH/CYCLOD",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $202A
         jsr   $2000      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $8075      ; patch - don't decrease lives
+
         +DISABLE_ACCEL
         jmp   $8EA6

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/RAMPAGE",plain
*=$106

    !source "src/prelaunch/common.a"

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         php
         inc   $3F4       ; force reboot
         lda   #$60
         sta   $413A
         jsr   $2EED
         plp
         beq   +
         lda   #$b1
         sta   $A3DC      ; patch - don't decrease lives
+
         jmp   $E800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/ANTIAIR",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$4C
         sta   $955A
         jsr   $8000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $4C76      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $955D

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

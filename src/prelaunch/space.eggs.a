!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPACE.EGGS",plain
*=$106

    !source "src/macros.a"

         +NEW_RESET_VECTOR $280
         +ENABLE_ACCEL

         lda   #$60
         sta   $2056
         jsr   $2000      ; decompress

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         dec   $1BBA      ; don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $4880      ; run it

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

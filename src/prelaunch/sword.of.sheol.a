!cpu 6502
!to "build/PRELAUNCH.INDEXED/SWORD.OF.SHEOL",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $86A3
         jsr   $6300      ; decompress
         +RESET_VECTOR $100

         inc   $70        ; uninitialised variable
         
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $6BDE      ; patch - don't decrease armour
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $50F

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/GRAPPLE",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $3F06
         jsr   $3EBA      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         dec   $7A92      ; don't count deaths
+
         lda   #1
         sta   $40D3      ; reset vector fix

         +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

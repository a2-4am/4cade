!cpu 6502
!to "build/PRELAUNCH.INDEXED/ROADBLOCK",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3192
         jsr   $800       ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $15D0      ; patch - don't decrease time
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE

         jmp   $80D

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/ROADBLOCK",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3192
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $15D0      ; patch - don't decrease time
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $810

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/DIG.DUG",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $826
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$24
         sta   $a1d9
         sta   $8ca6
+
         +DISABLE_ACCEL
         jmp   $8000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

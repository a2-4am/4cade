!cpu 6502
!to "build/PRELAUNCH.INDEXED/DIG.DUG",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $826
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$24
         sta   $a1d9
         sta   $8ca6
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $8000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/ARKANOID",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $97e
         jsr   $800

         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$b5
         sta   $145c
         sta   $1d65
+
         lda   $c083
         jsr   DisableAccelerator
         lda   $c08b
         jsr   $ee1f

         +LC_REBOOT
         lda   #$2C
         sta   $600
         sta   $603       ; BIT changing reset

         jmp   $981

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

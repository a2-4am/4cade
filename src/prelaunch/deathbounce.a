!cpu 6502
!to "build/PRELAUNCH.INDEXED/DEATHBOUNCE",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $949
         jsr   $900       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #0
         sta   $6F44      ; don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $7A42

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

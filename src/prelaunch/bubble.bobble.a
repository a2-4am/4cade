!cpu 6502
!to "build/PRELAUNCH.INDEXED/BUBBLE.BOBBLE",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $978
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$b5
         sta   $82b0
         sta   $8324
+
         +READ_ROM_WRITE_RAM1
         lda   #<reset
         sta   $3F2
         sta   $FFFC
         lda   #>reset
         sta   $3F3
         sta   $FFFD
         eor   #$A5
         sta   $3F4
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $6000

reset
         +READ_ROM_NO_WRITE
         jmp   $FAA6

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

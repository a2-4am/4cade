!cpu 6502
!to "build/PRELAUNCH.INDEXED/PM",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60       ; hook1
         sta   $3EA9
         jsr   $3E88      ; decompress
         +DISABLE_ACCEL

         lda   #$60       ; hook2
         sta   $A050
         jsr   $A000      ; title/load

         lda   #$60       ; hook3
         sta   $948
         jsr   $940

         +RESET_AND_IRQ_VECTOR $100

         ldx   #0
         stx   $10AD
         inx
         stx   $10B2

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         dec   $107B      ; patch - don't decrease lives
+
         jmp   $950

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/NM",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8C36
         jsr   $44E5      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #HAS_JOYSTICK
         bne   +
         inc   $AFA       ; force keyboard
+        +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $0800

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

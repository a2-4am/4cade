!cpu 6502
!to "build/PRELAUNCH.INDEXED/NM",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8C36
         jsr   $44E5      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #HAS_JOYSTICK
         bne   +
         inc   $AFA       ; force keyboard
+        jsr   DisableAccelerator
         jsr   HideLaunchArtworkLC2
         +READ_ROM_NO_WRITE
         jmp   $0800

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

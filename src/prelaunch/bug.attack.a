!cpu 6502
!to "build/PRELAUNCH.INDEXED/BUG.ATTACK",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #>(callback-1)
         sta   $641A
         lda   #<(callback-1)
         sta   $641D
         jmp   $6400      ; decompress

callback
         +DISABLE_ACCEL
         jmp   $4D36

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

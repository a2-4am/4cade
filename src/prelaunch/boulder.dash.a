!cpu 6502
!to "build/PRELAUNCH/BOULDER.DASH",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $404A
         jsr   $4000      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$b5
         sta   $181A      ; patch - don't decrease lives
+
         +DISABLE_ACCEL
         jmp   $800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

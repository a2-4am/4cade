!cpu 6502
!to "build/PRELAUNCH.INDEXED/CHIP.OUT",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $603D
         jsr   $6000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $0B71
+
         +DISABLE_ACCEL_LC
         jmp   $200

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

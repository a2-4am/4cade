!cpu 6502
!to "build/PRELAUNCH.INDEXED/CHIP.OUT",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $604B
         jsr   $6000      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $0B71
+
         +DISABLE_ACCEL
         jmp   $200

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/LODE.RUNNER",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1F4E
         jsr   $1F00      ; decompress
         dec   $6AD1
         inc   $6ADA      ; swap buttons
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $9050

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

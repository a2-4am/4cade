!cpu 6502
!to "build/PRELAUNCH/ALCAZAR",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4035
         jsr   $1ffd      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $8289      ; patch - don't decrease lives
+
         lda   #$4C
         sta   $9DBF
         lda   #0
         sta   $9DC0
         lda   #1
         sta   $9DC1      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $7c1e

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

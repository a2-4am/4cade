!cpu 6502
!to "build/PRELAUNCH.INDEXED/SI",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $401A
         jsr   $4000      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6B18      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         jsr   HideLaunchArtworkLC2
         +READ_ROM_NO_WRITE

         jmp   $6000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

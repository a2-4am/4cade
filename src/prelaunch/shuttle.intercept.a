!cpu 6502
!to "build/PRELAUNCH.INDEXED/SI",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $401A
         jsr   $4000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $6B18      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC

         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

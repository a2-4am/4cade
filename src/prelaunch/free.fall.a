!cpu 6502
!to "build/PRELAUNCH.INDEXED/FREE.FALL",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $204E
         jsr   $2000      ; decompress
         lda   #$4C
         sta   $320
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
                          ; lives in $1532
         lda   #$AD
         sta   $0E19      ; patch - don't decrease lives
         sta   $19F2      ; patch - don't decrease lives
         sta   $0E69      ; patch - don't increase lives
         sta   $7E11      ; patch - don't increase lives
         sta   $BE11      ; patch - don't increase lives
+
         +RESET_VECTOR $100
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $4000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

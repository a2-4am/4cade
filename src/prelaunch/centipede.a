!cpu 6502
!to "build/PRELAUNCH/CENTIPEDE",plain
*=$106

         !source "src/prelaunch/common.a"

         +USES_TEXT_PAGE_2
         +ENABLE_ACCEL
         lda   #$60
         sta   $0C33
         jsr   $0C00      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$B5
         sta   $A231      ; patch - don't decrease lives
+
         +DISABLE_ACCEL
         jmp   $A000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/TSUNAMI",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$4C
         sta   $7703
         jsr   $6BEC      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $17A8      ; patch - don't decrease lives
         sta   $1916      ; patch - don't increase lives
         sta   $1761      ; patch - don't decrease fuel
+
         +DISABLE_ACCEL_LC
         jmp   $A0ED

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

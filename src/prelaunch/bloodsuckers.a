!cpu 6502
!to "build/PRELAUNCH.INDEXED/BLOODSUCKERS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $950B
         jsr   $59E8       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $1B11       ; patch - don't decrease weapons
         sta   $8B8A       ; patch - don't decrease health
+
         +DISABLE_ACCEL_LC
         jmp   $7E03

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

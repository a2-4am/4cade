!cpu 6502
!to "build/PRELAUNCH/BURGERTIME",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3ec9
         sta   $3f09
         jsr   $3e97
         +DISABLE_ACCEL
         lda   #$b1
         sta   $646b
         jsr   $6400
         +ENABLE_ACCEL
         jsr   $811
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$24
         sta   $a433
         sta   $96ea
         sta   $94d1
         sta   $9768
+
         lda   #0
         sta   $A30A
         lda   #1
         sta   $A30F      ; reset vector patch
         
         +DISABLE_ACCEL
         jmp   $a300

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

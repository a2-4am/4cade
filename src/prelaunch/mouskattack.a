!cpu 6502
!to "build/PRELAUNCH.INDEXED/MOUSKATTACK",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$4C
         sta   $810
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $58AC      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $813

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

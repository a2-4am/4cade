!cpu 6502
!to "build/PRELAUNCH/ALIEN.MUNCHIES",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1638
         jsr   $1610      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $10c9
         sta   $be8
+
         +DISABLE_ACCEL
         jmp   $833

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

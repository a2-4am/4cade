!cpu 6502
!to "build/PRELAUNCH/GUARDIAN",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $934F
         jsr   $7400      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $5799      ; patch - don't decrease lives
+        +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/MIEYEN",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$4C
         sta   $5FEE
         jsr   $43E4
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $6334      ; don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/RAINBOW.ZONE",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $93D0
         jsr   $6B00      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $5A32       ; patch - don't decrease fuel
         sta   $5CFA       ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $801

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

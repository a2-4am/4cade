!cpu 6502
!to "build/PRELAUNCH.INDEXED/RAINBOW.ZONE",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $93D0
         jsr   $6B00      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $5A32       ; patch - don't decrease fuel
         sta   $5CFA       ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $801

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

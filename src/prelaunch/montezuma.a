!cpu 6502
!to "build/PRELAUNCH.INDEXED/MZ",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $816
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $6d0d
+
         +DISABLE_ACCEL
         jmp   $6004

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

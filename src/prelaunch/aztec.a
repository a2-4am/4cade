!cpu 6502
!to "build/PRELAUNCH.INDEXED/AZTEC",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL_AND_HIDE_ARTWORK
         lda   #$60
         sta   $1F3C
         jsr   $1E60      ; decompress
         lda   #$20
         sta   $6571
         lda   #<keypress
         sta   $6572
         lda   #>keypress
         sta   $6573
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$AD
         sta   $61B5
         sta   $61BD
         sta   $61C3
         sta   $61D0
         sta   $52D7
         sta   $144B
+
         +DISABLE_ACCEL_LC
         jmp   $1B00

keypress
         lda   KBD
         bpl   keypress
         cmp   #$8D
         bne   keypress
         rts

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

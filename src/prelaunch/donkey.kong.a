!cpu 6502
!to "build/PRELAUNCH/DONKEY.KONG",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3eab
         jsr   $3e36      ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$2c
         sta   $729b
         sta   $6fea
+
         lda   #0
         sta   $4112
         lda   #1
         sta   $4117      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

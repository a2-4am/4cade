!cpu 6502
!to "build/PRELAUNCH.INDEXED/SC",plain
*=$106

    !source "src/prelaunch/common.a"

         jmp   +

callback
         +ENABLE_ACCEL
         jsr   $BD8E
         +RESET_VECTOR reset
         +DISABLE_ACCEL
         lda   #0
         rts

reset
         +READ_RAM1_NO_WRITE
         jmp   ($FFFC)

+
         lda   #<callback
         sta   $8BF
         lda   #>callback
         sta   $8C0
         jmp   $800      ; decompress

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/SS.BASKETBALL",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9EB
         jsr   $800      ; decompress

         inc   $3F4      ; reboot

         +DISABLE_ACCEL
         jmp   $B700     ; could probably hook further

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

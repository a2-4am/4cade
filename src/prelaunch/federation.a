!cpu 6502
!to "build/PRELAUNCH.INDEXED/FEDERATION",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1C7C
         jsr   $1C00      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $A63       ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $D566

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

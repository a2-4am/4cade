!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPACE.QUARKS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3E9D
         jsr   $3E65      ; decompress/set reset vector

         +RESET_VECTOR $100
         +DISABLE_ACCEL

         jmp   $200

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/PENGO",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $206C
         jsr   $2000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $1269      ; patch - don't decrease lives
         sta   $14CC      ; patch - don't increase lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $800

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

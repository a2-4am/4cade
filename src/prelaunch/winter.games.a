!cpu 6502
!to "build/PRELAUNCH/WINTER.GAMES",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9EB
         jsr   $800      ; decompress

         +READ_RAM1_WRITE_RAM1
         +LC_REBOOT
         +READ_ROM_NO_WRITE

         +DISABLE_ACCEL
         jmp   $B700

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/SPUTNIK.ATTACK",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$4C
         sta   $93D9
         jsr   $6900      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $6B37
+
         +DISABLE_ACCEL

-        lda   $C000
         bpl   -
         lda   $C010
         jmp   $8A60

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

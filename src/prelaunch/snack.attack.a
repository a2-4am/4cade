!cpu 6502
!to "build/PRELAUNCH/SNACK.ATTACK",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $82E
         jsr   $800       ; decompress
         +DISABLE_ACCEL

         jsr   $BD1C      ; title screen

         lda   #$60
         sta   $BD19
         jsr   $BCFA

         ; re-patch reset vector
         +RESET_VECTOR $100

         jmp   $7E00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/SNACK.ATTACK",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #$60
         sta   $82E
         jsr   $800       ; decompress
         jsr   $BD1C      ; title screen

         lda   #$60
         sta   $BD19
         jsr   $BCFA

         lda   #0
         sta   $3F2
         lda   #1
         sta   $3F3
         lda   #$A4
         sta   $3F4       ; reset vector patch

         jmp   $7E00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/PENETRATOR",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $82D
         jsr   $800       ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         dec   $7602      ; patch - don't decrease lives
+
         +DISABLE_ACCEL
         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

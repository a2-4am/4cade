!cpu 6502
!to "build/PRELAUNCH/MATING.ZONE",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $6B18
         sta   $49D4
         jsr   $495E      ; decompress
         jsr   $BD41
         +DISABLE_ACCEL
         jsr   $B59B

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $729F      ; patch - don't decrease lives
+
         jmp   $BF8

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

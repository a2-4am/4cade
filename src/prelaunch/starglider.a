!cpu 6502
!to "build/PRELAUNCH.INDEXED/STARGLIDER",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $81FC
         jsr   $800       ; decompress

         jsr   DisableAccelerator
         jsr   $BA7B

         ldx   #0
         stx   $7323
         inx
         stx   $732B
         inc   $101
         lda   #$A6
         sta   $104
         lda   #$FA
         sta   $105       ; update reset hook to reboot

         jmp   $BA00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

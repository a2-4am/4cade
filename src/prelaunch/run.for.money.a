!cpu 6502
!to "build/PRELAUNCH.INDEXED/RUN.FOR.MONEY",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $897
         jsr   $800       ; decompress
         ldx   #0
         stx   $43B3
         inx
         stx   $43B8      ; reset vector
         +DISABLE_ACCEL

         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/DEPTH.CHARGE",plain
*=$106

    !source "src/macros.a"

reset
         +ENABLE_ACCEL
         +NEW_RESET_VECTOR reset
         lda   #$60
         sta   $1A67
         jsr   $1A00      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         dec   $BFB       ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE

         jmp   $D566

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

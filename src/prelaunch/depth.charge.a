!cpu 6502
!to "build/PRELAUNCH.INDEXED/DEPTH.CHARGE",plain
*=$106

    !source "src/macros.a"

reset
         +ENABLE_ACCEL
         +NEW_RESET_VECTOR reset
         lda   #$60
         sta   $1A67
         jsr   $1A00      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         dec   $BFB       ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC

         jmp   $D566

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

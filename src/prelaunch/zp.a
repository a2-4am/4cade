!cpu 6502
!to "build/PRELAUNCH.INDEXED/ZP",plain
*=$106

         !source "src/macros.a"

         +HIDE_ARTWORK
         +NEW_RESET_VECTOR $180

         lda   #$60
         sta   $3076
         sta   $313B
         jsr   $800       ; decompress some
         +ENABLE_ACCEL
         ldy   #$28
         jsr   $3288
         jsr   $3079      ; decompress more
         +DISABLE_ACCEL
         jmp   $D566

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

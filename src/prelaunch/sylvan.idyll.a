!cpu 6502
!to "build/PRELAUNCH.INDEXED/SYLVAN.IDYLL",plain
*=$106

    !source "src/macros.a"

         lda   #$60
         sta   $33C
         jsr   $300
         +ENABLE_ACCEL
         lda   #$60
         sta   $24F6
         jsr   $2400      ; decompress
         lda   #$8D
         sta   $24F6
         bit   $c083
         jsr   DisableAccelerator
         jmp   $24F6

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/RG",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9413
         jsr   $7000      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $FE2       ; patch - don't increase lives
         sta   $1A65      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $803

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

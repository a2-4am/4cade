!cpu 6502
!to "build/PRELAUNCH.INDEXED/MUTANT",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $205F
         jsr   $2000      ; decompress title
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $8774       ; patch - don't decrease lives
+

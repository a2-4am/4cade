!cpu 6502
!to "build/PRELAUNCH.INDEXED/MUTANT",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $205F
         jsr   $2000      ; decompress title
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $8774       ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $8A73

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

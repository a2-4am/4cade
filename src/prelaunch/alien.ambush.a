!cpu 6502
!to "build/PRELAUNCH.INDEXED/ALIEN.AMBUSH",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $81c
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         dec   $5ff5
         dec   $608b
+
         +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

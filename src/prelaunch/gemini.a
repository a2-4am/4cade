!cpu 6502
!to "build/PRELAUNCH.INDEXED/GEMINI",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $7E2A
         jsr   $6200       ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $D0C        ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $D566

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

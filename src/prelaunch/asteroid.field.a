!cpu 6502
!to "build/PRELAUNCH/ASTEROID.FIELD",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $5CDA
         jsr   $4000      ; decompress

         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $14E5      ; patch - don't decrease lives
+
         lda   #1
         sta   $12F2
         +DISABLE_ACCEL
         jmp   $4000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/LEMMINGS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $21A5
         jsr   $2000      ; decompress

         +READ_RAM2_WRITE_RAM2
         +NEW_RESET_VECTOR_64K $300
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $6000

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

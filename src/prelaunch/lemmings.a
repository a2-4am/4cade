!cpu 6502
!to "build/PRELAUNCH/LEMMINGS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $41B0
         jsr   $4000      ; decompress
         inc   $C9        ; RNG requires non-zero
         +DISABLE_ACCEL
         jmp   $8C8

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

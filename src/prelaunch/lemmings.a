!cpu 6502
!to "build/PRELAUNCH.INDEXED/LEMMINGS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $21A5
         jsr   $2000      ; decompress

         +READ_RAM2_WRITE_RAM2
         +NEW_RESET_VECTOR_64K $300
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE

         jmp   $6000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

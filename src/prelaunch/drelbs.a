!cpu 6502
!to "build/PRELAUNCH/DRELBS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $813
         jsr   $800       ; decompress
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $1cb0      ; patch - don't decrease lives
+
         +READ_RAM2_WRITE_RAM2
         +LC_REBOOT

         +DISABLE_ACCEL
         jmp   $8000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

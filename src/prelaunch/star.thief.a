!cpu 6502
!to "build/PRELAUNCH.INDEXED/STAR.THIEF",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $5F12
         jsr   $3A55      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$bd
         sta   $6B31      ; patch - don't decrease lives
         sta   $6DC2      ; patch - don't decrease lives
         sta   $70F2      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $6801

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

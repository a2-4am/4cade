!cpu 6502
!to "build/PRELAUNCH/SPINDIZZY",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         +READ_RAM1_WRITE_RAM1
         +LC_REBOOT
         +READ_ROM_NO_WRITE

         lda   #$60
         sta   $855
         jsr   $800       ; decompress

         ldx   #0
         stx   $9344
         inx
         stx   $934F      ; fix reboot

         +DISABLE_ACCEL
         sta   $C05F      ; fix //c and //c+ DHGR setting
         jmp   $9300

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

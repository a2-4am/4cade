!cpu 6502
!to "build/PRELAUNCH/ALIEN.TYPHOON",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $472f
         sta   $4739
         jsr   $4710
         +DISABLE_ACCEL
         jsr   $6000
         +ENABLE_ACCEL
         jsr   $806
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         dec   $7270
         dec   $7b53
+
         lda   #0
         sta   $2067
         lda   #1
         sta   $206C      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $2001

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/BOLO",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8034
         jsr   $4ab0      ; decompress and show title
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $1619
         sta   $11ae

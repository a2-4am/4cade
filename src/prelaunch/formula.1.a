!cpu 6502
!to "build/PRELAUNCH/FORMULA.1",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $818
         jsr   $800

         lda   #1
         sta   $71D4      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $7000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

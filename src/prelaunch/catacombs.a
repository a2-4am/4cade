!cpu 6502
!to "build/PRELAUNCH.INDEXED/CATACOMBS",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #<callback
         sta   $24A9
         lda   #>callback
         sta   $24AA
         ldy   #(x_e - x_b) - 1
-        lda   x_b, y
         sta   $200, y
         dey
         bpl   -
         jmp   $200

x_b !pseudopc $200 {
         jmp   $2400      ; decompress

callback
         sta   $c008
         +RESET_VECTOR $1A6A
         bit   $c083
         lda   #$8b
         sta   $BF0E
         jsr   DisableAccelerator
         bit   $c08b
         sta   $c009
         jmp   $800
}
x_e

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

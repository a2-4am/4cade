!cpu 6502
!to "build/PRELAUNCH/IP",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $813
         jsr   $800       ; decompress

         lda   #$2C
         sta   $1806
         sta   $180B
         sta   $1810
         sta   $1833
         sta   $1838
         sta   $183D      ; reset vector fix

         +DISABLE_ACCEL
         jmp   $1800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
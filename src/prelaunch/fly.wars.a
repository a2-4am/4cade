!cpu 6502
!to "build/PRELAUNCH.INDEXED/FLY.WARS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $8B28
         jsr   $55FD      ; decompress
         lda   #$60
         sta   $5A6
         jsr   $5A0       ; decompress more

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $7C32      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_AND_HIDE_ARTWORK_LC
         jmp   $5500

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/TELEPORT",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $549F
         jsr   $52D1      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $41D1      ; patch - don't decrease lives
         sta   $507C      ; patch - don't increase lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $803

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

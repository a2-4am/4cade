!cpu 6502
!to "build/PRELAUNCH.INDEXED/WILLY.BYTE",plain
*=$106

    !source "src/macros.a"

         +READ_RAM2_WRITE_RAM2
         jsr   EnableAccelerator
         jsr   HideLaunchArtworkLC2
         +LOAD_FILE_AT willybyte_mb, $D000
         +READ_ROM_NO_WRITE
         ldx   #(accel_e - accel)
-        lda   accel, x
         sta   callback1, x
         dex
         bpl   -
         lda   #<callback1
         sta   $414A
         lda   #>callback1
         sta   $414B
         lda   #$60
         sta   $426B
         jsr   $4000      ; decompress
         lda   #$20
         sta   $7852
         lda   #<callback2
         sta   $7853
         lda   #>callback2
         sta   $7854
         lda   #$EA
         sta   $7855
         lda   #$4C
         sta   $B6D8
         ldx   #0
         stx   $B6D9
         inx
         stx   $B6DA      ; reset vector
         jmp   $6000

callback2
         sbc   $8A
         cmp   #8
         beq   +
         cmp   #9
+        rts

accel !pseudopc $bf80 {
callback1
         +READ_RAM2_NO_WRITE
         jsr   EnableAccelerator
         jsr   $BD80      ; decompress
         jsr   $D000
         jsr   DisableAccelerator
!if *!=$bf8f {
  !error "callback4 moved ",*
}
callback4
         jsr   $fca8
         +READ_ROM_NO_WRITE
         lda   #0         ; set Z
         rts
}
accel_e

willybyte_mb
         +PSTRING "PRELAUNCH/WILLY.BYTE.MB"

!if * > $1bf {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/WILLY.BYTE",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $426B
         jsr   $4000      ; decompress
         lda   #<callback
         sta   $BD7C
         lda   #>callback
         sta   $BD7D
         lda   #$4C
         sta   $B6D8
         ldx   #0
         stx   $B6D9
         inx
         stx   $B6DA      ; reset vector
         +DISABLE_ACCEL
         jmp   $6000

callback
         +ENABLE_ACCEL
         jsr   $BD80      ; decompress
         +DISABLE_ACCEL
         lda   #0         ; set Z
         rts

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

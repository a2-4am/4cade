!cpu 6502
!to "build/PRELAUNCH.INDEXED/MEGABOTS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9F1
         jsr   $800       ; load DOS

         +DISABLE_ACCEL
         ldx   #$60
         jmp   $B700

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

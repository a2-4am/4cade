!cpu 6502
!to "build/PRELAUNCH.INDEXED/PB",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $833
         sta   $85A
         jsr   $800       ; decompress
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jsr   $85D
         +ENABLE_ACCEL
         jsr   $836

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $158D      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $7BC8

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

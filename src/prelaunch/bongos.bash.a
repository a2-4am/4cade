!cpu 6502
!to "build/PRELAUNCH.INDEXED/BH",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2063
         jsr   $2000      ; decompress

         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $84F5      ; patch - don't decrease lives
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $D566

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/BH",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2060
         jsr   $2000      ; decompress

         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A5
         sta   $84F5      ; patch - don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $D566

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

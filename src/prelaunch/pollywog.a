!cpu 6502
!to "build/PRELAUNCH.INDEXED/POLLYWOG",plain
*=$106

    !source "src/macros.a"

         +USES_TEXT_PAGE_2
         +ENABLE_ACCEL
         lda   #$60       ; RTS instead of JMP
         sta   $846
         jsr   $800       ; decompress
         lda   #$D0
         sta   $5D2A
         sta   $5DA5      ; don't touch $C040/annunciator strobe
         +DISABLE_ACCEL
         jsr   HideLaunchArtwork
         jmp   $7957

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

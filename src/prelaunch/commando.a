!cpu 6502
!to "build/PRELAUNCH.INDEXED/COMMANDO",plain
*=$106

         jmp   ++

cheat
         jsr   EnableAccelerator ; yes!
         jsr   $d33b
         jsr   DisableAccelerator
         lda   $d4bd
         eor   #$c5
         bne   +
         sta   $1053
         sta   $1063
+        rts

++
    !source "src/macros.a"

         +ENABLE_ACCEL
         dec   $11a6
         jsr   $1000      ; decompress
         jsr   HideLaunchArtworkLC2
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         lda   #<cheat
         sta   $d312
         lda   #>cheat
         sta   $d313
+
         jsr   DisableAccelerator
         jmp   $803

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

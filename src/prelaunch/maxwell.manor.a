!cpu 6502
!to "build/PRELAUNCH.INDEXED/MAXWELL.MANOR",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #<callback
         sta   $21C8
         lda   #>callback
         sta   $21C9
         jmp   $800        ; decompress

callback
         +READ_RAM2_WRITE_RAM2
         +LC_REBOOT
         jsr   DisableAccelerator
         jsr   HideLaunchArtworkLC2
         jmp   $21CD

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/TROMPERS",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $2025
         jsr   $2000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$ad
         sta   $f60
+

         +DISABLE_ACCEL_LC
         jmp   $7700

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

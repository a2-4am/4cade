!cpu 6502
!to "build/PRELAUNCH.INDEXED/WALL.DEFENCE",plain
*=$106

         !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $2013
         jsr   $2000      ; decompress

         ldx   #0
         stx   $D01
         inx
         stx   $D06       ; reset fix

         +DISABLE_ACCEL
         jmp   $D00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH.INDEXED/PAIPEC",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4031
         jsr   $4000      ; decompress
         lda   #<callback
         sta   $682C
         lda   #>callback
         sta   $682D
         +DISABLE_ACCEL
         jmp   $1AD0
callback
         sta   $C010      ; clear keypress
         jmp   $100       ; don't reboot

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

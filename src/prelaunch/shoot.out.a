!cpu 6502
!to "build/PRELAUNCH.INDEXED/SHOOT.OUT",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $202E
         jsr   $2000      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         lda   #$A9
         sta   $8C3       ; don't decrease lives
+
         +DISABLE_ACCEL_LC
         jmp   $800

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/PHARAOHS.REVNG",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $3D61
         jsr   $3AFE      ; decompress

         lda   MachineStatus
         and   #CHEATS_ENABLED
         php
         jsr   DisableAccelerator
         +LC_REBOOT
         lda   #$60
         sta   $85F
         jsr   $862
         jsr   $844
         plp
         beq   +
         lda   #$a5
         sta   $CF7       ; patch - don't decrease lives
+
         lda   #1
         sta   $AC6
         jmp   $A00

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

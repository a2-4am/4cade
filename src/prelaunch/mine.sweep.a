!cpu 6502
!to "build/PRELAUNCH.INDEXED/MINE.SWEEP",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1F21
         jsr   $1F00      ; decompress
         +READ_RAM2_NO_WRITE
         lda   MachineStatus
         and   #CHEATS_ENABLED
         beq   +
         dec   $1134       ; patch - don't decrease time
+
         jsr   DisableAccelerator
         +READ_ROM_NO_WRITE
         jmp   $800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

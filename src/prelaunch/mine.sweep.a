!cpu 6502
!to "build/PRELAUNCH.INDEXED/MINE.SWEEP",plain
*=$106

    !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $1F21
         jsr   $1F00      ; decompress
         +GET_MACHINE_STATUS_LC
         and   #CHEATS_ENABLED
         beq   +
         dec   $1134       ; patch - don't decrease time
+
         +DISABLE_ACCEL_LC
         jmp   $800

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}

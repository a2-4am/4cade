!cpu 6502
!to "build/PRELAUNCH.INDEXED/STARMINES",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $6013
         jsr   $6000      ; decompress
         ldx   #0
         stx   $1FC9
         inx
         stx   $1FCE      ; reset fix
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $800

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}

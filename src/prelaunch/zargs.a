!cpu 6502
!to "build/PRELAUNCH.INDEXED/ZARGS",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #>(callback-1)
         sta   $51BE
         lda   #<(callback-1)
         sta   $51C1
         jmp   $800       ; decompress

callback
         lda   #1
         sta   $60B0      ; reset vector fix
         +DISABLE_ACCEL
         jmp   $6000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

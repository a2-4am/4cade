;------------------------------------------------------------------------------
; OnQuit
; reboot system
;
; in:    none
; out:   nothing
;------------------------------------------------------------------------------
OnQuit
         jsr   SwitchToBank2
         jsr   EnableAcceleratorAndSwitchToBank1
         inc   $101       ; bank in ROM
         +LDADDR ROM_REBOOT
         +ST16 $104
         jmp   $100
